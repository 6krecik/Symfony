<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Nasze demo{% endblock %}</title>
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
    <link rel="stylesheet" href="{{ asset('bundles/adesignscalendar/css/fullcalendar/fullcalendar.css') }}" />
    <link rel="stylesheet" href="{{ asset('bundles/tor/css/bootstrap.min.css') }}" />
    <link rel="stylesheet" href="{{ asset('bundles/tor/css/style.css') }}" />
    {#<script type="text/javascript" src="{{ asset('bundles/adesignscalendar/js/jquery/jquery-1.8.2.min.js') }}"></script>#}
    <script type="text/javascript" src="{{ asset('bundles/tor/js/jquery-1.11.3.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/adesignscalendar/js/fullcalendar/jquery.fullcalendar.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/tor/js/calendar-settings.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/tor/js/bootstrap.min.js') }}"></script>
    {% block stylesheets %}{% endblock %}


    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

</head>
<body>
<nav class="navbar navbar-default ">
    <div class="container">
        {% if is_granted('ROLE_ADMIN') %}
            {{ knp_menu_render('TorBundle:Builder:adminMenu') }}
        {% elseif is_granted('ROLE_USER') %}
            {{ knp_menu_render('TorBundle:Builder:mainMenu') }}
        {% elseif is_granted('IS_AUTHENTICATED_ANONYMOUSLY') %}
            {{ knp_menu_render('TorBundle:Builder:loginMenu') }}
        {% endif %}
</div>

</nav>

{% block body %}{% endblock %}

</body>

</html>